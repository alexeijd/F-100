<?xml version="1.0" encoding="UTF-8"?>

<!--F-100D tachometer -->

<PropertyList>

<path>Models/tach.ac</path>
<texture-path>Textures</texture-path>

<!--LIGHTING-->

<animation>
	<type>material</type>
	<object-name>indicator</object-name>
	<emission>
		<red>0.75</red>
		<green>0.25</green>
		<blue>0.25</blue>
		<factor-prop>/controls/lighting/panel-norm</factor-prop>
	</emission> 
</animation> 

<!--ROTATIONS-->

<animation>
	<type>rotate</type>
	<object-name>needle1</object-name>
	<property>/engines/engine[0]/n1</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>10</ind><dep>27</dep></entry>
		<entry><ind>20</ind><dep>52</dep></entry>
		<entry><ind>30</ind><dep>78</dep></entry>
		<entry><ind>40</ind><dep>107</dep></entry>
		<entry><ind>50</ind><dep>135</dep></entry>
		<entry><ind>60 </ind><dep>162</dep></entry>
		<entry><ind>70</ind><dep>189</dep></entry>
		<entry><ind>80</ind><dep>216</dep></entry>
		<entry><ind>90</ind><dep>242</dep></entry>
		<entry><ind>100</ind><dep>270</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
	</axis>
</animation>

<animation>
	<type>rotate</type>
	<object-name>needle2</object-name>
	<property>/engines/engine[0]/n1</property>
	<factor>36</factor>
	<center>
		<x-m>0</x-m>
		<y-m>-0.011028</y-m>
		<z-m>0.012008</z-m>
		</center>
	<axis>
		<x>-1</x>
	</axis>
</animation>

<!--LoD-->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>

