<?xml version="1.0"?>

<!--F-100D loadmeter-->

<PropertyList>

<path>Models/loadmeter2.ac</path>
<texture-path>Textures</texture-path>

<!--LIGHTING-->

<animation>
	<type>material</type>
	<object-name>indicator</object-name>
	<emission>
		<red>0.75</red>
		<green>0.25</green>
		<blue>0.25</blue>
		<factor-prop>/controls/lighting/panel-norm</factor-prop>
	</emission> 
</animation> 

<!--METAL EFFECTS-->

<animation>
	<type>shader</type>
	<shader>chrome</shader>
	<texture>metal-shader.png</texture>
		<object-name>screws</object-name>
</animation>

<!--LoD-->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>
