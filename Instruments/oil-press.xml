<?xml version="1.0"?>

<!--F-100D oil pressure indicator-->

<PropertyList>

<path>Models/oil-press.ac</path>
<texture-path>Textures</texture-path>

<!--LIGHTING-->

<animation>
	<type>material</type>
	<object-name>indicator</object-name>
	<emission>
		<red>0.75</red>
		<green>0.25</green>
		<blue>0.25</blue>
		<factor-prop>controls/lighting/panel-norm</factor-prop>
	</emission> 
</animation> 

<!--ROTATION-->

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>/engines/engine[0]/oil-pressure-psi</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>20</ind><dep>60</dep></entry>
		<entry><ind>40</ind><dep>120</dep></entry>
		<entry><ind>50</ind><dep>142.5</dep></entry>
		<entry><ind>60</ind><dep>165</dep></entry>
		<entry><ind>70</ind><dep>187.5</dep></entry>
		<entry><ind>80</ind><dep>210</dep></entry>
		<entry><ind>90</ind><dep>232.5</dep></entry>
		<entry><ind>100</ind><dep>255</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
	</axis>
</animation>

<!--LoD-->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>