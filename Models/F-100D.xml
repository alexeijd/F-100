<!-- landing and nav lights-->

<animation>
	<type>select</type>
	<object-name>landing-lights-on</object-name>
	<condition>
		<and>
			<equals>
				<property>/controls/gear/landing-lights</property>
				<value>1</value>
			</equals>
			<greater-than>
				<property>/controls/gear/landing-lights-pos-norm</property>
				<value>0</value>
			</greater-than>
		</and>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>landing-lights-off</object-name>
	<condition>
		<or>
			<equals>
				<property>/controls/gear/landing-lights</property>
				<value>0</value>
			</equals>
			<equals>
				<property>gear/gear[0]/position-norm</property>
				<value>0</value>
			</equals>
		</or>
	</condition>
</animation>

<animation> 
	<type>rotate</type>
	<object-name>landing-lights</object-name>
	<property>/controls/gear/landing-lights-pos-norm</property>
	<factor>90</factor>
	<center> 
		<x-m>-2.529672</x-m>
		<y-m>0</y-m>
		<z-m>-0.703408</z-m>
	</center>
	<axis> 
		<y>1</y>
	</axis>
</animation>

